<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hollybits.uberblockchain">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registration"
            />
        <activity android:name=".MainAccount" />
        <activity android:name=".AllParcelOrders" />
        <activity android:name=".MakeOrderActivity" />
        <activity android:name=".MySentParcels" />
        <activity android:name=".LikeDriverIMustToDeliver" />
        <activity android:name=".LikeUserIMustToDeliver"></activity>
    </application>

</manifest>